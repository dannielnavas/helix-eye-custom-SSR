{
  "name": "helix-eye",
  "version": "'",
  "description": "SSR React app",
  "main": "index.js",
  "author": "Danniel Navas",
  "license": "MIT",
  "scripts": {
    "build-client": "webpack --config webpack.config.client.js",
    "build-server": "webpack --config webpack.config.server.js",
    "build": "NODE_ENV=development pnpm run build-client && pnpm run build-server",
    "build-dev": "NODE_ENV=development pnpm run build-server && node --inspect dist/index.js",
    "dev": "nodemon --watch ts --exec pnpm run build-dev",
    "start": "NODE_ENV=development pnpm build && NODE_ENV=development node dist/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.7.3",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^3.1.4",
    "react-dev-utils": "^12.0.1",
    "swc-loader": "^0.2.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^7.3.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-node-externals": "^3.0.0"
  }
}
